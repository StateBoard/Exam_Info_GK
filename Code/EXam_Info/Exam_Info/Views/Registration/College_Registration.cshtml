@model Exam_Info.Models.CollegeRegistrationValidation

@{
    ViewBag.Title = "College_Registration";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}
<h2>
    <center>
        <b>
            College Registration
        </b>
    </center>
</h2>

<style>
    label {
        font-size: 15px !important;
        font-weight: bold;
    }
</style>
<form id="submit_form">
    <div class="container-fluid page-body-wrapper full-page-wrapper">
        <div class="content-wrapper d-flex align-items-stretch auth auth-img-bg">
            <div class="auth-form-transparent text-left p-3">
                <br />
                <div class="form-group">
                    <div class="row">
                        <div class="col-sm-4">
                            <label for="Index No">Index No</label><span style="color:Red">*</span>
                            @Html.TextBoxFor(model => model.Index_No, new { @class = "form-control", @placeholder = "Index No", @name = "Index_No", @required = "required", maxLength = 7 })
                            @Html.ValidationMessageFor(model => model.Index_No, "", new { @class = "text-danger" })

                        </div>
                        <div class="col-sm-4">
                            <label for="College Name">College Name</label><span style="color:Red">*</span>
                            @Html.TextBoxFor(model => model.College_Name, new { @class = "form-control", @placeholder = "College Name", @name = "College_Name", @required = "required" })
                            @Html.ValidationMessageFor(model => model.College_Name, "", new { @class = "text-danger" })
                        </div>
                        <div class="col-sm-4">
                            <label for="College Addres">College Address</label><span style="color:Red">*</span>
                            @Html.TextBoxFor(model => model.College_Address, new { @class = "form-control", @placeholder = "College Address", @name = "College_Name", @required = "required" })
                            @Html.ValidationMessageFor(model => model.College_Address, "", new { @class = "text-danger" })
                        </div>

                    </div>
                </div>
                <br />

                <div class="row">

                    <div class="col-sm-4">

                        <label for="Principal Name">Principal Name</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.Principal_Name, new { @class = "form-control", @placeholder = "Principal Name", @name = "Principal_Mobile ", maxLength = 10, @required = "required" })
                        @Html.ValidationMessageFor(model => model.Principal_Name, "", new { @class = "text-danger" })

                    </div>


                    <div class="col-sm-4">
                        <label for="Principal_Mobile">PrincipalMobile</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.Principal_Mobile, new { @class = "form-control", @placeholder = "Principal Mobile ", @name = "Principal_Name ", @required = "required" })
                        @Html.ValidationMessageFor(model => model.Principal_Mobile, "", new { @class = "text-danger" })
                    </div>
                </div>

                <br />

                <div class="row">
                    <div class="col-sm-4">
                        <label for="Total Students">Total Students</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.Total_Students, new { @class = "form-control", @placeholder = "Total Students", @name = "Total_Students ", maxLength = 4 })
                        @Html.ValidationMessageFor(model => model.Total_Students, "", new { @class = "text-danger" })
                    </div>
                    <div class="col-sm-4">
                        <label for="Total Machines">Total Machines</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.Total_Machines, new { @class = "form-control", @placeholder = "Total Machines", @name = "Total_Machines ", maxLength = 4 })
                        @Html.ValidationMessageFor(model => model.Total_Machines, "", new { @class = "text-danger" })
                    </div>
                    <div class="col-sm-4">
                        <label for="Total Teachers">Total Teachers</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.Total_Teachers, new { @class = "form-control", @placeholder = "Total Teachers", @name = "Total_Teachers ", maxLength = 4 })
                        @Html.ValidationMessageFor(model => model.Total_Teachers, "", new { @class = "text-danger" })

                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-sm-4">
                        <label for="IT Teacher Name"> Teacher Name</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.IT_Teacher_Name, new { @class = "form-control", @placeholder = " Teacher Name", @name = "IT_Teacher_Name " })
                        @Html.ValidationMessageFor(model => model.IT_Teacher_Name, "", new { @class = "text-danger" })
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-sm-4">
                        <label for="IT Teacher Mobile No.1"> Teacher MobileNo.1</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.IT_Teachers_MobileNumber1, new { @class = "form-control", @placeholder = " Teachers MobileNumber1", @name = "IT_Teachers_MobileNumber1 ", maxLength = 10 })
                        @Html.ValidationMessageFor(model => model.IT_Teachers_MobileNumber1, "", new { @class = "text-danger" })
                    </div>
                    <div class="col-sm-4">
                        <label for="IT Teachers MobileNumber2"> Teacher MobileNo.2</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.IT_Teachers_Mobilenumber2, new { @class = "form-control", @placeholder = " Teachers Mobilenumber2", @name = "IT_Teachers_Mobilenumber2 ", maxLength = 10 })
                        @Html.ValidationMessageFor(model => model.IT_Teachers_Mobilenumber2, "", new { @class = "text-danger" })
                    </div>
                </div>
                <br />
                <div class="row">
                    <div class="col-sm-4">
                        <label for="Password">Password</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.Password, new { @class = "form-control", @placeholder = "Password", @name = "Password ", @type = "Password" })
                        @Html.ValidationMessageFor(model => model.Password, "", new { @class = "text-danger" })

                    </div>
                    <div class="col-sm-4">
                        <label for="Confirm_Password">Confirm Password</label><span style="color:Red">*</span>
                        @Html.TextBoxFor(model => model.Confirm_Password, new { @class = "form-control", @placeholder = "Confirm Password", @name = "Confirm_Password" })
                        @Html.ValidationMessageFor(model => model.Confirm_Password, "", new { @class = "text-danger" })
                    </div>
                </div>
                <br />
                <div class="col-sm-2">
                    <center>
                        <input type="button" id="btn_submit" class="btn btn-block btn-primary btn-lg font-weight-bolder auth-form-btn" value="SUBMIT" onclick="return confirm('Do you want to save ?')" style="background-color: #3eadcf;font-weight:bold">
                    </center>
                </div>
            </div>
        </div>
    </div>
</form>



<script>
    $(document).ready(function () {

        $("#btn_submit").click(function (e) {
            debugger
                      var formData = new FormData($(submit_form)[0]);

                      $.ajax({
                          url: "Save_College_Registration",
                          type: "POST",
                          data: formData,
                          processData: false,
                          contentType: false,
                          beforeSend: function () {
                              Showloader();
                          },
                          complete: function () {
                              Hideloader();
                          },
                          success: function (Data) {
                              if (Data.Result) {
                                  Success_Alert(Data.Response);
                              }
                              else {
                                  Error_Alert(Data.Response);
                              }

                          },
                          error: function (e1, e2, e3) {
                              Error_Alert(Data.Response);
                          }
                      });
                  })

    })</script>