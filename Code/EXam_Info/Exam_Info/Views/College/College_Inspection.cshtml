@model IEnumerable<Exam_Info.Models.Tbl_Inspection>
@{
    ViewBag.Title = "College_Inspection";
    Layout = "~/Views/Shared/College_LayoutPage.cshtml";
}



<style>
    table, th, td {
        border: 1px solid black;
        border-collapse: collapse;
    }
    #btn_submit {
        width: 200px;
        padding: 5px;
        position: absolute;
        right: 20px;
    }
</style>

<div class="row">
    <div class="col-sm-6">
        <h2>College Inspection</h2>
        </div>
    <div class="col-sm-6">
        @*<input type="button" class="btn btn-success" id="btn_submit" value="Download Excel" onclick="return confirm('Do you want to download ?')"><br />*@
        <button class="btn btn-success" id="btn_submit" onclick="return confirm('Do you want to download ?')">@Html.ActionLink("Download Excel", "ExportToExcelV", "College", new { controller = "College" })</button><br />
        @*<button style="padding:10px;margin-top:5px;margin-bottom:5px;">@Html.ActionLink("DOWNLOAD", "ExportToExcelV", "College", new { controller = "College" })</button>*@
    </div>
</div>

    <div class="row" style="border:inset 2px  #3eadcf"></div>

    <br />

    <div class="table-responsive pt-3  ">
        <center>
            <table class="AppFormTable " style="background-color: #e7fafe;">
                <tr>
                    
                    <th>
                        Index No
                    </th>
                    <th>
                        System No
                    </th>
                    <th>
                        OS Name
                    </th>
                    <th>
                        RAM
                    </th>
                    <th>
                        HDD
                    </th>
                    <th>
                        MAC
                    </th>
                    <th>
                        Browser Name
                    </th>
                    <th>
                        External IP
                    </th>
                    <th>
                        Screen Res
                    </th>
                    <th>
                        IE Version
                    </th>
                    <th>
                        Active
                    </th>

                </tr>
              
                @foreach (var item in Model)
                {
                    <tr>
                        
                        <td>
                            @Html.DisplayFor(model => item.Index_No)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.SYS_No)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.OS_Name)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.Ram)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.HDD)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.MAC)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.Browser_Name)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.Extn_IP)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.Screen_Res)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.IE_Version)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.Active)
                        </td>
                    </tr>
                }
            </table>
        </center>
    </div>

